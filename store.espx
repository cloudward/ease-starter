<# include "_htmlheader.espx" #>

<style>



.storeqty{
	width: 20px;
}
.storelistingcontainer{
	width: 100%;
}
.storelisting2{
	width: 250px;
	min-height: 100px;
	margin: 10px;
	padding: 5px;
	border: 1px #CCC solid;
	border-radius: 3px;
	box-shadow: 2px 2px 2px #666;
	display: inline-block;
	background-color: #efefef;
}
.storelistingimagecontainer{
	width: 100%;
	text-align: center;
	background-color: #FFF;
	border-radius: 3px;
}
.storelistingdesccontainer{
	width: 100%;
}
.storeimagesmall2{
	margin-left: auto;
	margin-right: auto;
	background-color: #FFF;
	width: 100px;
	margin-top: 10px;
	margin-bottom: 10px;
}

.storelistingname{
	margin-top: 10px;
}
.storelistingpricecontainer{
	width: 100%;
	background-color: #FFF;
}
.storelistingprice{
	min-width: 32%;
	height: 15px;
	padding: 5px;
	background: #1bbc9b; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #1bbc9b 0%, #13836b 82%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#1bbc9b), color-stop(82%,#13836b)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #1bbc9b 0%,#13836b 82%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #1bbc9b 0%,#13836b 82%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #1bbc9b 0%,#13836b 82%); /* IE10+ */
	background: linear-gradient(to bottom,  #1bbc9b 0%,#13836b 82%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1bbc9b', endColorstr='#13836b',GradientType=0 ); /* IE6-8 */


	
	color: #FFF;
	font-size: 18px;
	font-weight: bold;
	text-align: right;
	margin-right: -12px;
}

.storelistingform{
	text-align: right;
	margin: 5px 0px 3px 0px;
}
.triangle{
	float: right;
	margin-right: -12px;
	width: 0px;
	height: 0px;
	border-style: solid;
	border-width: 12.5px 0 12.5px 12px;
	border-color: transparent transparent transparent #ffffff;
	line-height: 0px;
	_border-color: #000000 #000000 #000000 #ffffff;
	_filter: progid:DXImageTransform.Microsoft.Chroma(color='#000000');
}
</style>


<div style="width: 100%;text-align: right;">
	<div style="width: 30%;text-align: right;float: right;">
		<input type="button" onclick="javascript: window.location='/?page=store_cart&orderid=<#[cookie.orderid]#>';" value="View My Cart">
	</div>
	<div style="clear: both;"></div>
</div>
<div class="storelistingcontainer">
	<# start list for content;
		include when content.page == "store-top";

		hide pager both;
	#>
	<# start header #><# end header #>
	<# start row #>
	<# content.body #>
	<# end row #>
	<# start footer #><# end footer #>
	<# no results #>
		<div class="warning">Store-top Not Setup <a href="/?page=admin_wizard_pages&edit=0&iid=3a48923809754c7ca77cd72db6fcb403&value=store-top&name=Website-store-top&desc=Content for our store-top"><u>Click here</u></a>.</div>
	<# end no results #>
	<# end list #>

	<# start list for googlespreadsheet <#[webstyle.store_sheet_id]#>;

		include all columns from "Sheet1" where row.a != "" and row.f != "";

		start row template at row 2;

	#>
	<# start header #>
		<div class="storeheader"></div>
	<# end header #>

	<# start row #>
		<div class="storelisting2">
			<div class="storelistingpricecontainer">
				<div class="storelistingprice">$ <# row.f #></div>
				<div class="triangle"></div>
				<div style="clear: both;"></div>
			</div>
			<div class="storelistingimagecontainer">
				<img src="<# row.c #>" border="0" class="storeimagesmall2">
				<div style="clear: both;"></div>
			</div>
			<div class="storelistingdesccontainer">
				<div class="storelistingname"><# row.a #></div>
				<div class="storelistingdesc"><# row.b #>
				<div class="storelistingsku">sku <# row.g #></div>
					<form name="addtocart" action="/" method="get" class="storelistingform"> 
						<input type="hidden" value="store_add_to_cart" name="page">
						<input type="text" value="1" name="qty" maxlength="5" class="storeqty">
						<input type="submit" value="Add to Cart" class="storebutton">
						<input type="hidden" value="<# row.a #>" name="product_name">
						<input type="hidden" value="<# row.f #>" name="price">
						<input type="hidden" value="<# row.g #>" name="sku">
						<input type="hidden" value="<# row.c #>" name="img">
						<input type="hidden" value="<# row.h #>" name="tax_rate">
						<input type="hidden" value="<# row.i #>" name="shipping_charge">
					</form>
				</div>
				<div style="clear: both;"></div>
			</div>
			<div style="clear: both;"></div>
		</div>
	<# end row #>

	<# start footer #>
		<div class="storefooter"></div>
	<# end footer #>

	<# end list #>
	
	<# start list for content;
		include when content.page == "store-bottom";
		hide pager both;
	#>
	<# start header #><# end header #>
	<# start row #>
	<# content.body #>
	<# end row #>
	<# start footer #><# end footer #>
	<# no results #>
		<div class="warning">Store-bottom Not Setup <a href="/?page=admin_wizard_pages&edit=0&iid=46b241a7a08b4e57a5f7e548836bc864&value=store-bottom&name=Website-store-bottom&desc=Content for our store-bottom"><u>Click here</u></a>.</div>
	<# end no results #>
	<# end list #>
	<div style="clear: both;"></div>
</div>


<# include "_htmlfooter.espx" #>
